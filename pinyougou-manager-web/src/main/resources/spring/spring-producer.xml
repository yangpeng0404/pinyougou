<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--消息生产者-->
    <!--指定端口，以及生产组名-->
    <bean class="org.apache.rocketmq.client.producer.DefaultMQProducer" id="defaultMQProducer" init-method="start" destroy-method="shutdown">
        <property name="namesrvAddr" value="192.168.25.129:9876"></property>
        <property name="producerGroup" value="producer_manager_web_group"></property>
    </bean>

</beans>